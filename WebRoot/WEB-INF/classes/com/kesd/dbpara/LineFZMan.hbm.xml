<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.kesd.dbpara.LineFZMan" table="line_fzman">
		<id name="id" column="id" type="short">
			<generator class="increment"></generator>
		</id>
		
		<property name="describe" column="describe" type="string"></property>
		<property name="orgId" column="org_id" type="short"></property>
		<property name="telNo1" column="telno1" type="string"></property>
		<property name="telNo2" column="telno2" type="string"></property>
		<property name="addr" column="addr" type="string"></property>
		
	</class>
</hibernate-mapping>