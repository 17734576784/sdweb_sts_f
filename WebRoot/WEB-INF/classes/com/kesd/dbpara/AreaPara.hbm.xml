<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.kesd.dbpara.AreaPara" table="areapara">
		<id name="id" column="id" type="integer">
			<generator class="increment"></generator>
		</id>
		<property name="orgId" 		column="org_id" 	type="short"></property>
		<property name="describe" 	column="describe" 	type="string"></property>
		<property name="areaCode" 	column="area_code" 	type="string"></property>
		
		<property name="feeprojId" 		column="feeproj_id" 	type="short"></property>
		<property name="yffalarmId" 	column="yffalarm_id" 	type="short"></property>
		<property name="feeBegindate" 	column="fee_begindate" 	type="integer"></property>
		
		<property name="remark" 	column="remark" 	type="string"></property>
		<property name="infCode1" 	column="inf_code1" 	type="string"></property>
		<property name="infCode2" 	column="inf_code2" 	type="string"></property>
		<property name="infCode3" 	column="inf_code3" 	type="string"></property>
		<property name="reserve1" 	column="reserve1" 	type="string"></property>	
	</class>
</hibernate-mapping>