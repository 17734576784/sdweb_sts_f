<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.kesd.dbpara.HoLiDayDef" table="holidaydef">
		<id name="id" column="id" type="short">
			<generator class="increment"></generator>
		</id>
		
		<property name="describe" column="describe" type="string"></property>
		<property name="sdate" column="sdate" type="integer"></property>
		<property name="reserve1" column="reserve1" type="string"></property>
		<property name="reserve2" column="reserve2" type="string"></property>
	</class>
</hibernate-mapping>