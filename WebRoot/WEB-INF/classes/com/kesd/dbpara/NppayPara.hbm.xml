<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.kesd.dbpara.NppayPara" table="nppay_para">
		<composite-id>
			<key-property name="rtuId" column="rtu_id" type="integer"></key-property>
			<key-property name="mpId"  column="mp_id"  type="short"></key-property>
		</composite-id> 
		
		<property name="useFlag" 		column="use_flag" 		type="byte"></property>
		<property name="caclType" 		column="cacl_type" 		type="byte"></property>
		<property name="feectrlType" 	column="feectrl_type" 	type="byte"></property>
		<property name="payType" 		column="pay_type" 		type="byte"></property>
		<property name="yffmeterType" 	column="yffmeter_type" 	type="byte"></property>
		<property name="yffctrlType" 	column="yffctrl_type" 	type="byte"></property>
		<property name="feeprojId" 		column="feeproj_id"	 	type="short"></property>
		<property name="yffalarmId" 	column="yffalarm_id" 	type="short"></property>
		<property name="feeBegindate" 	column="fee_begindate" 	type="integer"></property>
		<property name="powLimit" 		column="pow_limit" 		type="integer"></property>
		<property name="nocycutFlag" 	column="nocycut_flag" 	type="byte"></property>
		<property name="nocycutMin" 	column="nocycut_min" 	type="short"></property>
		<property name="powerupProt" 	column="powerup_prot" 	type="byte"></property>
		<property name="keyVersion" 	column="key_version" 	type="byte"></property>
		<property name="cryplinkId" 	column="cryplink_id" 	type="byte"></property>
		
		<property name="payAdd1" 		column="pay_add1" type="double"></property>
		<property name="payAdd2" 		column="pay_add2" type="double"></property>
		<property name="payAdd3" 		column="pay_add3" type="double"></property>
		<property name="tzVal"	 		column="tz_val"   type="integer"></property>
		
		<property name="cbCycleType"	column="cb_cycle_type"  type="byte"></property>
		<property name="cbDayhour"	 	column="cb_dayhour" 	type="short"></property>
		<property name="jsDay"	 		column="js_day" 		type="byte"></property>
		
		<property name="reserve1" column="reserve1" type="integer"></property>
		<property name="reserve2" column="reserve2" type="integer"></property>
		<property name="reserve3" column="reserve3" type="integer"></property>
	</class>
</hibernate-mapping>