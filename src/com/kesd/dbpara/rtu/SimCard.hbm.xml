<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.kesd.dbpara.rtu.SimCard" table="simcard">
		<id name="id" column="id" type="short">
			<generator class="increment"></generator>
		</id>
		
		<property name="telNo" column="tel_no" type="string"></property>
		<property name="ipAddr" column="ip_addr" type="string"></property>
		<property name="serialNo" column="serial_no" type="string"></property>
		<property name="type" column="type" type="byte"></property>
		<property name="appType" column="app_type" type="string"></property>
		<property name="actualFlux" column="actual_flux" type="integer"></property>
		<property name="overrunFlux" column="overrun_flux" type="integer"></property>
		<property name="status" column="status" type="byte"></property>
		<property name="runDate" column="run_date" type="integer"></property>
		<property name="stopDate" column="stop_date" type="integer"></property>
		<property name="orgId" column="org_id" type="short"></property>
	</class>
</hibernate-mapping>