<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.kesd.dbpara.CsStand" table="cs_stand">
		<id name="id" column="id" type="short">
			<generator class="increment"></generator>
		</id>
		<property name="describe" 	column="describe" 	type="string"></property>
		<property name="stand" 		column="stand" 		type="short"></property>
		<property name="useFlag" 	column="use_flag" 	type="byte"></property>
		<property name="reserve1" 	column="reserve1" 	type="short"></property>
		<property name="reserve2" 	column="reserve2" 	type="integer"></property>
	</class>
</hibernate-mapping>